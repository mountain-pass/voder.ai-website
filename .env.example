# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env with real values to version control

# OpenAI Configuration (for AI features)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
VODER_AGENTIC_MODE=true

# Sentry Configuration (for error monitoring)
SENTRY_DSN=your_sentry_dsn_here

# Microsoft Clarity Configuration (for analytics)
CLARITY_PROJECT_ID=your_clarity_project_id_here
CLARITY_API_TOKEN=your_clarity_api_token_here

# GitLeaks License (for security scanning)
GITLEAKS_LICENSE=your_gitleaks_license_here

# Deployment Tokens (for CI/CD)
VERCEL_TOKEN=your_vercel_token_here
NETLIFY_AUTH_TOKEN=your_netlify_auth_token_here