{
  "type": "module",
  "name": "voder.ai-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "npm run test:ci",
    "dev": "vite",
    "prepare": "husky install",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview --port=4173 --strictPort",
    "test:e2e": "playwright test",
    "test:ci": "npm run build && npx playwright test --project=chromium --project=firefox --project=webkit",
    "test:performance": "./scripts/test-performance.sh",
    "lint": "eslint \"src/**/*.{js,ts,svelte}\"",
    "assert:lhci": "lhci autorun --config=lighthouserc.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint \"src/**/*.{js,ts,svelte}\" --fix",
    "lhci": "lhci autorun"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.6",
    "@fontsource/jetbrains-mono": "^5.2.6",
    "@sentry/browser": "^9.35.0",
    "@sentry/tracing": "^7.120.3",
    "gsap": "^3.13.0",
    "three": "^0.178.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.30.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.40.0",
    "@types/three": "^0.178.0",
    "beasties": "^0.3.5",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-define-config": "^2.1.0",
    "husky": "^9.1.7",
    "optipng-bin": "^9.0.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.6.2",
    "start-server-and-test": "^1.14.0",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.0",
    "vite-plugin-inline-source": "^4.0.0"
  },
  "engines": {
    "node": ">=22.17.0"
  },
  "description": "- `npm run dev`     Starts the Vite development server on port 5173. - `npm run build`     Compiles the production bundle into the `dist/` directory. - `npm run preview`",
  "main": "eslint.config.js",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
