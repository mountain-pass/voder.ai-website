{
  "type": "module",
  "name": "voder.ai-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview --port=4173 --strictPort",
    "test:e2e": "playwright test",
    "test:ci": "npm run build && start-server-and-test preview http://localhost:4173 test:e2e",
    "lint": "eslint \"src/**/*.{js,ts,svelte}\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint \"src/**/*.{js,ts,svelte}\" --fix",
    "lhci": "lhci autorun"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.6",
    "@fontsource/jetbrains-mono": "^5.2.6",
    "@sentry/browser": "^9.35.0",
    "@sentry/tracing": "^7.120.3",
    "gsap": "^3.13.0",
    "three": "^0.178.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.30.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.40.0",
    "@types/three": "^0.178.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-define-config": "^2.1.0",
    "optipng-bin": "^9.0.0",
    "prettier": "^3.6.2",
    "start-server-and-test": "^1.14.0",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.0"
  },
  "engines": {
    "node": ">=22.17.0"
  }
}
