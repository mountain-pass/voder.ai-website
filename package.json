{
  "type": "module",
  "name": "voder.ai-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "sync": "svelte-kit sync",
    "dev": "npm run sync && vite",
    "build": "npm run sync && vite build",
    "preview": "npm run sync && vite preview --port=4173 --strictPort",
    "test:e2e": "playwright test",
    "test:ci": "npm run build && start-server-and-test preview http://localhost:4173 test:e2e",
    "lint": "eslint \"src/**/*.{js,ts,svelte}\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint \"src/**/*.{js,ts,svelte}\" --fix"
  },
  "dependencies": {
    "@sveltejs/kit": "2.22.2",
    "@threlte/core": "8.0.5",
    "gsap": "^3.13.0",
    "svelte": "^5.23.1",
    "three": "^0.178.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.40.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@types/three": "^0.178.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-svelte": "^3.10.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "start-server-and-test": "^1.14.0",
    "svelte-eslint-parser": "^1.2.0",
    "svelte-preprocess": "^6.0.0",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
